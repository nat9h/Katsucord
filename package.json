{
  "name": "katsumi",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "nodemon --exec tsx src/index.ts",
    "deploy": "tsx src/utils/deploy-commands.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "discord",
    "discordjs",
    "bot discord",
    "discord bot"
  ],
  "author": "Natsyn",
  "license": "MIT",
  "description": "Discord Bot using Discord.js",
  "dependencies": {
    "@discordjs/rest": "^2.6.0",
    "@dotenvx/dotenvx": "^1.51.1",
    "discord-api-types": "^0.38.32",
    "discord.js": "^14.24.2",
    "mongoose": "^8.19.2",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@types/node": "^24.10.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  }
}
